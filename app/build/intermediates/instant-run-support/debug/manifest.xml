<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="solodovnik.autovin.com.myapplication"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="solodovnik.autovin.com.myapplication.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="solodovnik.autovin.com.myapplication.MainMenu" />
        <activity android:name="solodovnik.autovin.com.myapplication.About" />
        <activity android:name="solodovnik.autovin.com.myapplication.Contacts" />
        <activity android:name="solodovnik.autovin.com.myapplication.Abouts" />
        <activity android:name="solodovnik.autovin.com.myapplication.Full_inf" />
    </application>

</manifest>